<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.PostMapper">
	<insert id="create">
		INSERT INTO posts(
		user_id,
		title,
		body
		) VALUES (
		#{user_id},
		#{title},
		#{body}
		);
	</insert>
</mapper>